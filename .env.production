# WebGenie - Production Environment Configuration

# ==================== SERVER CONFIGURATION ====================
DEBUG=False
LOG_LEVEL=INFO

# ==================== DATABASE CONFIGURATION ====================
DB_USER=webgenie_prod
DB_PASSWORD=your_secure_password_here
DB_NAME=webgenie_prod_db
DB_HOST=postgres
DB_PORT=5432

# ==================== FRONTEND CONFIGURATION ====================
VITE_API_URL=https://your-domain.com/api/v1
NODE_ENV=production

# ==================== JOB CONFIGURATION ====================
MAX_CONCURRENT_JOBS=8
JOB_TIMEOUT_SECONDS=86400

# ==================== HUGGINGFACE CONFIGURATION ====================
HF_TOKEN=your_huggingface_token_here

# ==================== DOCKER CONFIGURATION ====================
USE_DOCKER=True
DOCKER_REGISTRY=grnbeeline

# ==================== PGADMIN CONFIGURATION ====================
PGADMIN_EMAIL=admin@your-domain.com
PGADMIN_PASSWORD=secure_password_here
